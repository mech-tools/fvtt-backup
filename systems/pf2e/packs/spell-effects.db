{"_id":"uJNDI3CGGOtcG8nB","data":{"description":{"value":"<p>Granted by <em>@Compendium[pf2e.spells-srd.IWUe32Y5k2QFd7YQ]{Gravity Weapon}</em> if your weapon deals 2 dice of damage.</p>"},"duration":{"expiry":"turn-start","sustained":false,"unit":"minutes","value":1},"level":{"value":1},"rules":[{"key":"ToggleProperty","label":"First weapon strike this round","property":"flags.pf2e.rollOptions.damage-roll.gravity-weapon"},{"key":"FlatModifier","label":"Gravity Weapon","predicate":{"all":["gravity-weapon"],"not":["unarmed"]},"selector":"mundane-damage","type":"status","value":4}],"source":{"value":""},"start":{"initiative":null,"value":0},"target":null,"tokenIcon":{"show":true},"traits":{"custom":"","rarity":{"value":"common"},"value":[]},"slug":"spell-effect-gravity-weapon-2-dice","schema":{"version":0.697,"lastMigration":null}},"effects":[],"flags":{"core":{"sourceId":"Compendium.pf2e.spell-effects.uJNDI3CGGOtcG8nB"}},"img":"systems/pf2e/icons/spells/gravity-weapon.webp","name":"Spell Effect: Gravity Weapon (2 Dice)","type":"effect"}
